    <script>

        // 成就頁面出現
        $(".ach").click(function(){
            $(".achivement").animate({
                right:'0vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
        });
        
        // 回前頁
        $(".backbt").click(function(){
            $(".achivement").animate({
                right:'-100vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
            $(".gift").animate({
                right:'-100vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
            $(".sdgs").animate({
                right:'-100vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
        });
        // 兌換頁面出現
        $(".gi").click(function(){
            $(".gift").animate({
                right:'0vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
        });
        // 公益頁面出現
        $(".sd").click(function(){
            $(".sdgs").animate({
                right:'0vw',
                height:'100%',
                width:'100vw'
            },"fast",function(){});
        });
    </script>

