                            $(".donate").click(function(){
                              $(".jq22").fadeIn(200);
                              $(".popup-wrap2").fadeIn(250);
                              
                                $('.hearT').toggleClass("heart-active");

                              $(".donatenum").val("");
                              $(".totleD").html("本月累計捐贈: 123km");
                              $('.canUsePoint').html("可使用里程: <strong style='color: rgb(232, 35, 35);''>155</strong> km");
                              
                              $(".heart-btn").fadeOut(1500);
                              event.stopPropagation();
                              return false;
                            })
                            $(".popup-close2").click(function() {
                              $(".popup-wrap2").fadeOut(250);
                                event.stopPropagation()
                            });
                          </script>

