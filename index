                          <script>
                            $(function() {
                            $( "#ok" ).click(function() {
                              $( "#ok" ).addClass( "onclic");
                              validate();
                            });

                            function validate() {
                              setTimeout(function() {
                                $( "#ok" ).removeClass( "onclic" );
                                $( "#ok" ).addClass( "validate2");
                              }, 1500 );
                              callback();
                            }
                              function callback() {
                                setTimeout(function() {
                                  $( "#ok" ).removeClass( "validate2" );
                                  $( "#ok" ).addClass( "validate2");
                                }, 1500 );
                                $('#ok').attr('disabled', true); //停用
                              }
                            });
                          </script>

                        
