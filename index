                <script>
                  for (let i = 0; i < document.querySelectorAll(".count").length; i++) {
                       for (let j = 0; j < 3; j++) {
                           let thisNumber = parseInt($(".data").eq(i).find(".count").eq(j).text())
                           let timeFlag = 0.01;
                           if (thisNumber < 100) {
                               timeFlag = 0.01;
                           }
                           if (thisNumber >= 100 && thisNumber < 500) {
                               timeFlag = 0.1;
                           }
                           countNuber(i, j, thisNumber, timeFlag);
                       }
                   }
                   
                   function countNuber(p, q, lastNum, timer) {
                       let m = 0;
                       setInterval(() => {
                           if (m <= lastNum) {
                               $(".data").eq(p).find(".count").eq(q).text(m++)
                           }
                       }, timer);
                   }
                </script>
                <!-- switch按鈕 -->
                <script type="text/javascript">
                  $(".on").hide();
                  var b=0;
                  $("#switch").click(function(){
                        $(".on").show();
                        $(".off").hide();
                        // $(".data").hide();
                      if($("input[id='switch']").prop("checked")){
                        $(".on").show();
                        $(".off").hide();
                        // 重新跑數字
                        for (let i = 0; i < document.querySelectorAll(".count").length; i++) {
                            for (let j = 0; j < 3; j++) {
                                let thisNumber = parseInt($(".data").eq(i).find(".count").eq(j).text())
                                let timeFlag = 0.01;
                                if (thisNumber < 100) {
                                    timeFlag = 0.01;
                                }
                                if (thisNumber >= 100 && thisNumber < 500) {
                                    timeFlag = 0.01;
                                }
                                countNuber(i, j, thisNumber, timeFlag);
                            }
                        }
                        
                        function countNuber(p, q, lastNum, timer) {
                            let m = 0;
                            setInterval(() => {
                                if (m <= lastNum) {
                                    $(".data").eq(p).find(".count").eq(q).text(m++)
                                }
                            }, timer);
                        }
                      }else{
                        $(".off").show();
                        $(".on").hide();
                        // 重新跑數字
                        for (let i = 0; i < document.querySelectorAll(".count").length; i++) {
                            for (let j = 0; j < 3; j++) {
                                let thisNumber = parseInt($(".data").eq(i).find(".count").eq(j).text())
                                let timeFlag = 0.01;
                                if (thisNumber < 100) {
                                    timeFlag = 5;
                                }
                                if (thisNumber >= 100 && thisNumber < 500) {
                                    timeFlag = 0.1;
                                }
                                countNuber(i, j, thisNumber, timeFlag);
                            }
                        }
                        
                        function countNuber(p, q, lastNum, timer) {
                            let m = 0;
                            setInterval(() => {
                                if (m <= lastNum) {
                                    $(".data").eq(p).find(".count").eq(q).text(m++)
                                }
                            }, timer);
                        }
                      }
                  })
                </script>

